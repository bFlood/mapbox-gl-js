<table class="documentation-methods">
    <tr>
        <th>Method</th>
        <th>Returns</th>
        <th>Description</th>
    </tr>
    {{#methods}}
    <tr>
        <td>{{>signature}}</td>
        <td>{{#returns}}&lt;{{type.name}}&gt; {{description}}{{/returns}}</td>
        <td>
            {{description}}
            {{#examples}}
            <h4>Code Example</h4>
            {% highlight js %}{{{.}}}{% endhighlight %}
            {{/examples}}
        </td>
    </tr>
    {{/methods}}
</table>
